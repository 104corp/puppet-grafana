{
  "name": "puppet-grafana",
  "version": "2.5.0",
  "author": "Vox Pupuli",
  "summary": "This module provides Grafana, a dashboard and graph editor for Graphite and InfluxDB.",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-grafana.git",
  "project_page": "https://github.com/voxpupuli/puppet-grafana",
  "issue_url": "https://github.com/voxpupuli/puppet-grafana/issues",
  "dependencies": [
    {
      "name": "garethr/docker",
      "version_requirement": ">= 3.5.0 <6.0.0"
    },
    {
      "name": "maestrodev/wget",
      "version_requirement": ">= 1.6.0 <2.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.5.1 <2.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=2.0.0 <3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    }
  ],
  "operatingsystem_support": [
